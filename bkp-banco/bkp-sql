CREATE  TABLE PRODUTO( 
       prod_id INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,
       prod_nome VARCHAR(100), 
       prod_descricao VARCHAR(255),
       prod_tarja VARCHAR(20),
       prod_data_fabric Date,
       prod_data_valid Date,
       prod_valor Float
);

INSERT INTO PRODUTO(prod_nome, prod_descricao, prod_tarja, prod_data_fabric, prod_data_valid, prod_valor) VALUES('Gardenal', 'teste', 'preta', '2015-04-12', '2014-04-14', 200);
